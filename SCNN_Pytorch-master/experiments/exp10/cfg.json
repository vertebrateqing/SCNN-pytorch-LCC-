{
  "device": "cuda:0",
  "MAX_EPOCHES": 30,

  "dataset": {
    "dataset_name": "CULane",
    "batch_size": 64,
    "resize_shape": [800, 288]
  },

  "optim": {
    "lr": 1e-2,
    "momentum": 0.9,
    "weight_decay": 1e-3,
    "nesterov": true
  },

  "lr_scheduler": {
    "warmup": 50,
    "max_iter": 8000
  }
}